{
  "base-toolbar/default": {
    "code": "'use client';\n\nimport { Popover, PopoverContent, PopoverTrigger } from '@/components/ui/base-popover';\nimport { Select, SelectContent, SelectItem, SelectTrigger, SelectValue } from '@/components/ui/base-select';\nimport { Toggle } from '@/components/ui/base-toggle';\nimport { ToggleGroup, ToggleGroupItem } from '@/components/ui/base-toggle-group';\nimport { Toolbar, ToolbarButton, ToolbarSeparator } from '@/components/ui/base-toolbar';\nimport {\n  AlignCenter,\n  AlignLeft,\n  AlignRight,\n  Bold,\n  Download,\n  Italic,\n  Redo,\n  Settings,\n  Share,\n  Underline,\n  Undo,\n} from 'lucide-react';\n\nconst fontSizes = [\n  { label: 'Small', value: '12px' },\n  { label: 'Medium', value: '14px' },\n  { label: 'Large', value: '16px' },\n  { label: 'Extra Large', value: '18px' },\n];\n\nexport default function ToolbarDemo() {\n  return (\n    <div className=\"w-full max-w-4xl mx-auto p-6 space-y-6\">\n      {/* Main Toolbar */}\n      <Toolbar>\n        {/* Text Formatting */}\n        <ToggleGroup toggleMultiple={true}>\n          <ToolbarButton\n            size=\"icon\"\n            render={\n              <ToggleGroupItem value=\"bold\" aria-label=\"Bold\">\n                <Bold className=\"h-4 w-4\" />\n              </ToggleGroupItem>\n            }\n          />\n          <ToolbarButton\n            size=\"icon\"\n            render={\n              <ToggleGroupItem value=\"italic\" aria-label=\"Italic\">\n                <Italic className=\"h-4 w-4\" />\n              </ToggleGroupItem>\n            }\n          />\n          <ToolbarButton\n            size=\"icon\"\n            render={\n              <ToggleGroupItem value=\"underline\" aria-label=\"Underline\">\n                <Underline className=\"h-4 w-4\" />\n              </ToggleGroupItem>\n            }\n          />\n        </ToggleGroup>\n\n        <ToolbarSeparator />\n\n        {/* Text Alignment */}\n        <ToggleGroup toggleMultiple={false}>\n          <ToolbarButton\n            size=\"icon\"\n            render={\n              <ToggleGroupItem value=\"left\" aria-label=\"Align left\">\n                <AlignLeft className=\"h-4 w-4\" />\n              </ToggleGroupItem>\n            }\n          />\n          <ToolbarButton\n            size=\"icon\"\n            render={\n              <ToggleGroupItem value=\"center\" aria-label=\"Align center\">\n                <AlignCenter className=\"h-4 w-4\" />\n              </ToggleGroupItem>\n            }\n          />\n          <ToolbarButton\n            size=\"icon\"\n            render={\n              <ToggleGroupItem value=\"right\" aria-label=\"Align right\">\n                <AlignRight className=\"h-4 w-4\" />\n              </ToggleGroupItem>\n            }\n          />\n        </ToggleGroup>\n\n        <ToolbarSeparator />\n\n        {/* Font Size */}\n        <Select indicatorPosition=\"right\">\n          <SelectTrigger className=\"w-24\">\n            <SelectValue placeholder=\"Size\" />\n          </SelectTrigger>\n          <SelectContent>\n            {fontSizes.map((size) => (\n              <SelectItem key={size.value} value={size.value}>\n                {size.label}\n              </SelectItem>\n            ))}\n          </SelectContent>\n        </Select>\n\n        <ToolbarSeparator />\n\n        {/* Undo/Redo */}\n        <ToolbarButton size=\"icon\" aria-label=\"Undo\">\n          <Undo className=\"h-4 w-4\" />\n        </ToolbarButton>\n        <ToolbarButton size=\"icon\" aria-label=\"Redo\">\n          <Redo className=\"h-4 w-4\" />\n        </ToolbarButton>\n\n        <ToolbarSeparator />\n\n        {/* Actions */}\n        <ToolbarButton size=\"icon\" variant=\"outline\" aria-label=\"Download\">\n          <Download className=\"h-4 w-4\" />\n        </ToolbarButton>\n        <ToolbarButton size=\"icon\" variant=\"outline\" aria-label=\"Share\">\n          <Share className=\"h-4 w-4\" />\n        </ToolbarButton>\n\n        {/* Settings */}\n        <Popover>\n          <ToolbarButton size=\"icon\" variant=\"outline\" render={<PopoverTrigger />} aria-label=\"Settings\">\n            <Settings className=\"h-4 w-4\" />\n          </ToolbarButton>\n          <PopoverContent className=\"w-64\">\n            <h3>Editor Settings</h3>\n            <div className=\"space-y-3 mt-3\">\n              <div className=\"flex items-center justify-between\">\n                <label className=\"text-sm font-medium\">Auto-save</label>\n                <Toggle aria-label=\"Toggle auto-save\" />\n              </div>\n              <div className=\"flex items-center justify-between\">\n                <label className=\"text-sm font-medium\">Spell check</label>\n                <Toggle aria-label=\"Toggle spell check\" />\n              </div>\n              <div className=\"flex items-center justify-between\">\n                <label className=\"text-sm font-medium\">Dark mode</label>\n                <Toggle aria-label=\"Toggle dark mode\" />\n              </div>\n            </div>\n          </PopoverContent>\n        </Popover>\n      </Toolbar>\n    </div>\n  );\n}\n",
